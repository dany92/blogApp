<table>
<thead>
<th>Title</th>
<th>Description</th>
<th>Time</th>
</thead>
<tbody>
<% obj.each do |a| %>
	<tr>
	<td><%=a.title%></td>
	<td><%=a.description%></td>
	<td>Created By: <%= a.user.username if a.user%>, 
	<%= time_ago_in_words(a.created_at) %> ago, 
	last updated: <%= time_ago_in_words(a.updated_at) %> ago </td>
	<td><%= link_to "Edit", edit_article_path(a.id) %></td>
	<td><%= link_to "Show", article_path(a.id) %></td>
	<td><%= link_to "Delete", article_path(a.id), :method => :delete %></td>
	</tr>
<%end%>
</tbody>
</table>